<Window x:Class="ParentControl.View.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParentControl.View"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <StackPanel>
        <Button Content="Назад" Click="BackToSignInWindow"></Button>
        <Label Content="Email"></Label>
        <TextBox Name="Email"></TextBox>
        <Label Content="Пароль"></Label>
        <TextBox Name="Password"></TextBox>
        <Label Content="Подтвердите введённый пароль"></Label>
        <TextBox Name="PasswordConfirmation"></TextBox>
        <Button Content="Зарегистрироваться" Click="Register"></Button>
    </StackPanel>
</Window>
